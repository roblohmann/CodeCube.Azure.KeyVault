<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Rob Lohmann</Authors>
    <Description>A straight-forward an simple wrapper around Azure Key Vault.</Description>
    <Copyright>Copyright 2019 Rob Lohmann</Copyright>
    <PackageProjectUrl>https://github.com/roblohmann/CodeCube.Azure.KeyVault</PackageProjectUrl>
    <RepositoryUrl>https://github.com/roblohmann/CodeCube.Azure.KeyVault</RepositoryUrl>
    <PackageTags>azure keyvault</PackageTags>
    <PackageIconUrl>https://github.com/roblohmann/CodeCube.Azure.KeyVault/blob/master/nuget-icon.png?raw=true</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.KeyVault" Version="3.0.4" />
    <PackageReference Include="Microsoft.Azure.Services.AppAuthentication" Version="1.3.0" />
  </ItemGroup>

</Project>
